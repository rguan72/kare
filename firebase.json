{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [ 
      { 
        "source": "/exp/**", 
        "dynamicLinks": true 
      } 
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
